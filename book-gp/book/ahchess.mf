    fill p;
endchar;
beginchar(25, 1em#, 1em#, 0); "Firzan - white";
    firzan_outline;
    draw p;
    draw z2 -- z12;
    draw z3 -- z11;
    fill px;
    fill pd;
    cullit;
    undrawdot z14;
    cullit;
endchar;
beginchar(26, 1em#, 1em#, 0); "Firzan - black";
    firzan_outline;
    fill p;
    cullit;
    undraw z2 -- z12;
    undraw z3 -- z11;
    cullit;
    unfill px;
    cullit;
