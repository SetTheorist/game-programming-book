curve_sidebar = round 1/18em;

%%%%%%%%%%%%%%%%%%%% ROOK %%%%%%%%%%%%%%%%%%%%
def rook_outline = 
    numeric q;
    q = round 1/12em;

    x1 = 1*q = w - x24;
    x2 = 1*q = w - x23;
    x3 = 2*q = w - x22;
    x4 = 2*q = w - x21;
    x5 = 3*q = w - x20;
    x6 = 3*q = w - x19;
    x7 = 2*q = w - x18;
    x8 = 2*q = w - x17;
    x9 = 4*q = w - x16;
    x10 = 4*q = w - x15;
    x11 = 5*q = w - x14;
    x12 = 5*q = w - x13;

    y1 = 1*q = y24;
    y2 = 2*q = y23;
    y3 = 2*q = y22;
    y4 = 3*q = y21;
    y5 = 4*q = y20;
    y6 = 8*q = y19;
    y7 = 9*q = y18;
    y8 = 11*q = y17;
    y9 = 11*q = y16;
    y10 = 10*q = y15;
    y11 = 10*q = y14;
    y12 = 11*q = y13;

    path p;
    p = z1 -- z2 -- z3 -- z4 -- z5 -- z6
        -- z7 -- z8 -- z9 -- z10 -- z11 -- z12
        -- z13 -- z14 -- z15 -- z16 -- z17 -- z18
        -- z19 -- z20 -- z21 -- z22 -- z23 -- z24
        -- cycle;

    pickup pencircle xscaled 0.03w yscaled 0.03w;% rotated 30;
    penlabels(range 1 thru 24);
enddef;
beginchar(0, 1em#, 1em#, 0); "Rook - mask";
    rook_outline;
    fill p;
endchar;
beginchar(1, 1em#, 1em#, 0); "Rook - white";
    rook_outline;
    draw p;
    draw z3 -- z22;
    draw z4 -- z21;
    draw z5 -- z20;
    draw z6 -- z19;
    draw z7 -- z18;
endchar;
beginchar(2, 1em#, 1em#, 0); "Rook - black";
    rook_outline;
    fill p;
    cullit;
    undraw z3 -- z22;
    undraw z4 -- z21;
    undraw z5 -- z20;
    undraw z6 -- z19;
    undraw z7 -- z18;
    cullit;
    draw p;
endchar;

%%%%%%%%%%%%%%%%%%%% CROCODILE %%%%%%%%%%%%%%%%%%%%
def crocodile_outline =
    numeric q;
    q = round 1/12em;

    x1 = 7*q;
    x2 = 8*q;
    x3 = 2*q;
    x4 = 2*q;
    x5 = 2.5*q;
    x6 = 6*q;
    x7 = 2*q;
    x8 = 3*q;
    x9 = 3*q;
    x10 = 6*q;
    x11 = 9*q;
    x12 = 11*q;
    x13 = 10.5*q;

    x14 = 8*q;
    x15 = 9*q;

    y1 = 1*q;
    y2 = 3*q;
    y3 = 3*q;
    y4 = 5*q;
    y5 = 4*q;
    y6 = 4*q;
    y7 = 7*q;
    y8 = 8.5*q;
    y9 = 7.5*q;
    y10 = 5.5*q;
    y11 = 6*q;
    y12 = 4*q;
    y13 = 1*q;

    y14 = 5*q;
    y15 = 4.5*q;

    path p;
    p = z1 -- z2 -- z3 -- z4 -- z5
        -- z6 -- z7 -- z8 -- z9
        -- z10 -- z11 .. z12 .. z13
        --cycle;

    pickup pencircle xscaled 0.03w yscaled 0.03w;% rotated 30;
    penlabels(range 1 thru 15);
enddef;
beginchar(3, 1em#, 1em#, 0); "Crocodile - mask";
    crocodile_outline;
    fill p;
endchar;
beginchar(4, 1em#, 1em#, 0); "Crocodile - white";
    crocodile_outline;
    draw p;
    draw z14 ..  z15;
endchar;
beginchar(5, 1em#, 1em#, 0); "Crocodile - black";
    crocodile_outline;
    fill p;
    cullit;
    undraw z14 ..  z15;
    cullit;
    draw p;
endchar;

